        addUser(recipient);
        emit Transfer(sender, recipient, tTransferAmount);
    }
    
    function random(uint256 max) private view returns (uint256) {
        uint256 randomnumber = uint256(keccak256(abi.encodePacked(block.timestamp,block.difficulty,msg.sender))) % max;
        return randomnumber;
     }


    function _lotteryEvent() private {	
	        uint256 _lotteryPools=tokenFromReflection(_lotteryPool);	
	        require(_lotteryPools >= _maxLotAmount, "insufficient Funds");	
	        require(Users.length >= 3, "Only Minimum Users Found");
	        removeAllFee();	
	        
	        getUpdatedBuckets();
	        
	        address top = getVerifiedWinner(TopTier.length(),1);	
	        address mid = getVerifiedWinner(MidTier.length(),2);	
	        address last = getVerifiedWinner(LastTier.length(),3);
	        uint256 totalTranfered=0;
	        
	        if(top != address(0)){
	            Winners.push(top);
	            _transferStandard(address(this),top,(_lotteryPools.mul(46)).div(100));	
	            totalTranfered=totalTranfered.add((_lotteryPools.mul(46)).div(100));
	        }
	        
           if(mid != address(0)){
	       	    Winners.push(mid);
	       	    _transferStandard(address(this),mid,(_lotteryPools.mul(30)).div(100));
	            totalTranfered=totalTranfered.add((_lotteryPools.mul(30)).div(100));
	       	}
	       	 
           if(last != address(0)){
	       		Winners.push(last);	
        	    _transferStandard(address(this),last,(_lotteryPools.mul(16)).div(100));
	              totalTranfered=totalTranfered.add((_lotteryPools.mul(16)).div(100));
	       	}
	       	
	        addgrandLotteryPool((_lotteryPools.mul(8).div(100)).mul(_getRate()));	
	         totalTranfered=totalTranfered.add((_lotteryPools.mul(8)).div(100));
	        _lotteryPools = _lotteryPools.sub(totalTranfered);	
	        _lotteryPool=_lotteryPools.mul(_getRate());	
	       
	        restoreAllFee();	
	}	
		
	function _grandFunds(uint256 _index) private {	
	        if (	
	            _grandpool[_index].balance > 0 &&	
	            block.timestamp > (_grandpool[_index].lastReward + 9 days)	
	        ) {	
	            removeAllFee();	
	           uint256 toTransfer = (_grandpool[_index].amount).mul(20).div(100);
	           if(toTransfer > (_grandpool[_index].balance)){
	               toTransfer=(_grandpool[_index].balance);
	           }
	            _transferStandard(	
	                address(this),	
	                _grandpool[_index].user,	
	                (toTransfer)	
	            );	
	            _grandpool[_index].balance = (_grandpool[_index].balance).sub(	
	                toTransfer	
	            );	
	            _grandpool[_index].lastReward = block.timestamp;	
	            restoreAllFee();	
	        }	
	  }	
		
	 function _grandlotteryEvent() private {	
	        require(tokenFromReflection(_grandlotteryPool) >= _maxgrandLotAmount, "insufficient Funds");
	        require(Users.length >= 3, "Only Minimum Users Found");
	        if (_grandpool.length > 0)	
	            require( _grandpool[_grandpool.length - 1].balance == 0, "Grand Lottery is Already Running");	
	        uint256 index = random(Users.length);
	        uint256 grandPoolamount=tokenFromReflection(_grandlotteryPool);
	        uint256 toTransfer = (grandPoolamount).mul(20).div(100);
	         removeAllFee();
	          _transferStandard(	
	                address(this),	
	                Users[index],	
	                (toTransfer)	
	            );
	           restoreAllFee();
	        _grandpool.push(	
	            grandWinPool({	
	                user: Users[index],	
	                amount: grandPoolamount,	
	                lastReward: block.timestamp,	
	                balance: grandPoolamount.sub(toTransfer)	
	            })	
	        );	
	        _grandWinners.push(Users[index]);	
	        _grandlotteryPool = 0;	
	   }
	   
	   function getVerifiedWinner(uint256 tierLength, uint256 Tier) private view returns(address){
	       if(tierLength > 0 && Tier == 1){
	            uint256 selectedTierBucket=random(tierLength);
	            uint256 selectedBucket=TopTier.at(selectedTierBucket);
	       uint256 lengthOf= _buckets[selectedBucket].length();
	       uint256 selectedUser=random(lengthOf);
	       address winner = _buckets[selectedBucket].at(selectedUser);
	        return winner;
	       }
	       else if(tierLength > 0 && Tier == 2){
	            uint256 selectedTierBucket=random(tierLength);
	            uint256 selectedBucket=MidTier.at(selectedTierBucket);
	       uint256 lengthOf= _buckets[selectedBucket].length();
	       uint256 selectedUser=random(lengthOf);
	       address winner = _buckets[selectedBucket].at(selectedUser);
	        return winner;
	       }else if(tierLength > 0 && Tier == 3){
	            uint256 selectedTierBucket=random(tierLength);
	            uint256 selectedBucket=LastTier.at(selectedTierBucket);
	       uint256 lengthOf= _buckets[selectedBucket].length();
	       uint256 selectedUser=random(lengthOf);
	       address winner = _buckets[selectedBucket].at(selectedUser);
	        return winner;
	       }else {
	           return address(0);
	       }
	      
	   }
	   
	   function getUpdatedBuckets()private{
	        //filter here by if buckets size > 0 then added to new enumset and maintain so that separate and select random will be easy
	         uint256 sum=0;
             bool low = true;
             bool middle=false;
             bool high = false;
             for(uint256 j=0 ; j < 100;j++){
                
                 if(_buckets[j].length() > 0){
                 sum = sum.add(_buckets[j].length());
                 
                if((sum.div(Users.length)).mul(100) <= 33 && low && j<=33){
                    LastTier.addNode(j);
                    TopTier.remove(j);
                    MidTier.remove(j);
                    if((sum.div(Users.length)).mul(100) >= 33  || j>=33){
                    sum=0;
                    low=false;
                    middle=true;
                    }
                   
                }else if((sum.div(Users.length)).mul(100) <= 33 && middle && j<=66){
                    MidTier.addNode(j);
                    TopTier.remove(j);
                    LastTier.remove(j);
                     if((sum.div(Users.length)).mul(100) >= 33  || j>=66){
                    sum=0;
                    middle=false;
                    high=true;
                    }
                }else {
                    TopTier.addNode(j);
                    MidTier.remove(j);
                    LastTier.remove(j);
                }
                
              }
             }
	   }
	    
	   function addBucket(address user) private{
	        bool isAdded=false;
	    uint256 eligibleBucket;
         for(uint256 j=0 ; j < 100;j++){
            bool check= balanceOf(user) >= _bucketscheck[j].min && balanceOf(user) < (_bucketscheck[j].max).add(1e9);
             if(check){
                 _buckets[j].addNode(user);
                 isAdded=true;
                 eligibleBucket=j;
             }else if(_buckets[j].contains(user)){
                  _buckets[j].remove(user);
             }
             if(isAdded && _buckets[j].contains(user) && j != eligibleBucket){
                 _buckets[j].remove(user);
                 break;
             }
             
         }
       }
       
       function addLocker(uint256 _amount, uint256 _interval, uint256 _dropPerMonth, address _to) public{
            _dropPerMonth = _amount.mul(_dropPerMonth).div(100);
            require(_interval >= 30 , "The Minimum interval should be 30 days");
           _lockers[_to]= dxLocker({
               amount : _amount,
               interval: (_interval).mul(1 days),
               dropPerMonth: _dropPerMonth,
               balance: _amount,
               isExists: true,
               lastReward: block.timestamp
           });
           removeAllFee();
           _transferStandard(msg.sender,address(this),_amount);
           _totalValueLocked=_totalValueLocked.add(_amount);
           restoreAllFee();
       }
       
       function withdrawFunds(address user) public{
            require(_lockers[user].isExists, "The User does'nt Exists !");
           if(_lockers[user].isExists){
               if(_lockers[user].balance > 0 && block.timestamp >= _lockers[user].lastReward.add(_lockers[user].interval)){
                    uint256 diffdays = ((block.timestamp.sub(_lockers[user].lastReward)).div(60).div(60).div(24));
                    uint256 remaining = diffdays.mod(30);
                    uint256 diff = diffdays.div(30);
                    uint256 pending = diff.mul(_lockers[user].dropPerMonth);
                    if(pending > _lockers[user].balance){
                        pending = _lockers[user].balance;
                    }
                    removeAllFee();
                    _transferStandard(address(this),user,(pending));
                    restoreAllFee();
                    _lockers[user].balance = (_lockers[user].balance).sub(pending);
                      if(remaining > 0){
                         _lockers[user].lastReward = (block.timestamp).sub((remaining).mul(1 days));
                    }else{
                         _lockers[user].lastReward = block.timestamp;
                    }
                   
               }
               
           }
            if(_lockers[user].balance <= 0){
             delete _lockers[user];
            }
       }
    
     
